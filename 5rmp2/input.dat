molecule mol {
  0 1
  O
  H 1 0.9
  H 1 0.9 2 104.5
  symmetry c1
}

plugin_load("./test.so")

set basis         sto-3g
set maxiter       50
set e_convergence 12

basisname = psi4.get_global_option("BASIS")
wfn = psi4.new_wavefunction(mol, basisname)
psi4.plugin('./test.so', wfn)

set mp2_type conv
energy('mp2')
