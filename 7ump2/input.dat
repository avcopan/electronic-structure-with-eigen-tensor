molecule mol {
  1 2
  O
  H 1 0.9
  H 1 0.9 2 104.5
  symmetry c1
}

plugin_load("./test.so")

set basis         sto-3g
set maxiter       50
set e_convergence 12
set reference     uhf
set scf_type      pk
set mp2_type      conv

basisname = psi4.get_global_option("BASIS")
wfn = psi4.new_wavefunction(mol, basisname)
psi4.plugin('./test.so', wfn)

energy('mp2')
